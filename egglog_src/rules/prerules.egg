(rewrite (Binop (Union) a b) (Binop (Union) b a))
(rewrite (Binop (Inter) a b) (Binop (Inter) b a))
(rewrite (Binop bop a b) (Fold bop (List (vec-of a b))))
(rewrite (Binop bop a (Fold bop list)) (Fold bop (Cons a list)))

